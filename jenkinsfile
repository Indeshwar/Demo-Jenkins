pipeline{
    agent{
        docker{
            image 'maven:3.8.1-adoptopenjdk-11'
        }
    }

    stages{
        stage('Clone the Project'){
            steps{
                git branch: 'main', url: 'https://github.com/Indeshwar/demo-java-jenkins'
            }
        }

        stage('Compliation'){
            steps{
                sh 'mvn clean install'
            }
        }
    }
}